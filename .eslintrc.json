{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "commonjs": true,
    "es6": true
  },
  "globals": {
    "__": true
  },
  "rules":{
    "indent": ["error", 2],
    "linebreak-style":"off",
    "no-use-before-define": "off",
    "react/button-has-type": "off"
  }
}
